{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": "Item that the user can buy."
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "column": {
        "unique": true
      }
    },
    "picture": {
      "type": "media"
    },
    "postal": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::postal-code.postal-code",
      "required": true
    }
  }
}
